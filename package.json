{
  "name": "blogging-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache ./src",
    "lint:fix": "eslint --cache --fix ./src",
    "format": "prettier \"./**/*.{js,jsx,ts,tsx,graphql,json,css}\" --list-different",
    "format:fix": "prettier \"./**/*.{js,jsx,ts,tsx,graphql,json,css}\" --list-different --write",
    "i18n:extract": "formatjs extract \"src/{pages,components,containers}/**/*.{js,jsx,ts,tsx}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file ./src/content/locales/cs.json",
    "i18n:compile": "formatjs compile-folder --ast --format simple ./src/content/locales ./src/content/locales-compiled",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.1",
    "@tailwindcss/forms": "^0.5.4",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "js-cookie": "^3.0.5",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.2",
    "react-markdown": "^8.0.7",
    "react-spinners": "^0.13.8",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.1.3",
    "@formatjs/intl": "^2.9.0",
    "@formatjs/intl-localematcher": "^0.4.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "20.4.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "10.4.14",
    "cypress": "^13.1.0",
    "dotenv": "^16.3.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "msw": "^1.3.0",
    "postcss": "8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react-intl": "^6.4.4",
    "ts-jest": "^29.1.1",
    "typescript": "5.1.6"
  }
}
